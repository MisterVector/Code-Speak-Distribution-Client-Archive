<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="637.0" prefWidth="722.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.codespeak.distribution.client.scenes.MainWindowController">
   <children>
      <AnchorPane prefHeight="580.0" prefWidth="722.0">
         <children>
            <MenuBar prefHeight="25.0" prefWidth="711.0">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Quit" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <TableView fx:id="programsTable" layoutX="14.0" layoutY="71.0" onMousePressed="#onProgramSelect" prefHeight="283.0" prefWidth="695.0">
              <columns>
                <TableColumn fx:id="programsTableNameColumn" prefWidth="315.0" text="Name" />
                <TableColumn fx:id="programsTableVersionColumn" prefWidth="153.0" text="Version" />
                  <TableColumn fx:id="programsTableReleaseTimeColumn" prefWidth="226.0" text="Release Time" />
              </columns>
            </TableView>
            <Label layoutX="14.0" layoutY="36.0" text="Select Category">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <CheckBox layoutX="374.0" layoutY="38.0" mnemonicParsing="false" text="Display Installed Programs Only">
               <font>
                  <Font size="14.0" />
               </font>
            </CheckBox>
            <AnchorPane layoutX="14.0" layoutY="362.0" prefHeight="316.0" prefWidth="695.0">
               <children>
                  <Label fx:id="programNameLabel" alignment="TOP_CENTER" layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="674.0" text="Select A Program" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
                  <Label fx:id="programDescriptionLabel" alignment="TOP_LEFT" layoutX="11.0" layoutY="76.0" prefHeight="195.0" prefWidth="674.0" text="No description. Select a program first." wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Button disable="true" layoutX="305.0" layoutY="277.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="93.0" text="Install" />
                  <Button disable="true" layoutX="11.0" layoutY="277.0" mnemonicParsing="false" text="Launch Program" />
                  <Button disable="true" layoutX="212.0" layoutY="277.0" mnemonicParsing="false" text="View Source" />
                  <Button disable="true" layoutX="123.0" layoutY="277.0" mnemonicParsing="false" text="Open Help" />
                  <Button disable="true" layoutX="409.0" layoutY="277.0" mnemonicParsing="false" text="Update" />
               </children>
            </AnchorPane>
            <ComboBox fx:id="categoryChoices" layoutX="124.0" layoutY="36.0" onAction="#onCategorySelect" prefHeight="25.0" prefWidth="237.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
